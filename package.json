{
  "name": "@makenew/jsmodule",
  "version": "4.0.1",
  "description": "Package skeleton for a JavaScript module.",
  "main": "dist/index.js",
  "module": "index.js",
  "sideEffects": false,
  "keywords": [
    "node"
  ],
  "homepage": "https://github.com/makenew/jsmodule",
  "bugs": "https://github.com/makenew/jsmodule/issues",
  "repository": "makenew/jsmodule",
  "license": "MIT",
  "author": {
    "name": "Evan Sosenko",
    "email": "razorx@evansosenko.com"
  },
  "files": [
    "yarn.lock",
    "index.js",
    "lib",
    "data",
    "dist",
    "docs"
  ],
  "scripts": {
    "test": "nyc ava",
    "test:update": "ava --update-snapshots",
    "test:watch": "ava --watch",
    "test:debug": "ava debug --break",
    "lint": "eslint --ignore-path .gitignore --ext .js .",
    "clean": "del dist",
    "build": "npm-run-all --parallel build:*",
    "prebuild": "npm-run-all clean",
    "build:index": "babel --source-maps --out-dir dist index.js",
    "build:lib": "babel --source-maps --ignore='**/*.spec.js' --out-dir dist/lib lib",
    "preversion": "npm-run-all lint test build",
    "postversion": "git push --follow-tags",
    "example": "babel-node examples",
    "example:inspect": "babel-node --inspect examples",
    "pretty": "prettier --ignore-path .gitignore --write '**/*.(js|json|graphql)'",
    "format": "npm-run-all --serial pretty format:*",
    "format:js": "eslint --ignore-path .gitignore --fix --ext .js .",
    "report": "nyc report"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@meltwater/examplr": "^5.0.0",
    "ava": "^3.3.0",
    "del-cli": "^3.0.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^16.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2",
    "standard": "^16.0.2"
  }
}
